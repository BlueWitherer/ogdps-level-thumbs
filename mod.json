{
    "geode": "3.4.0",
    "gd": {
        "win": "2.206",
        "android": "2.206",
        "mac": "2.206"
    },
    "version": "v1.3.0",
    "id": "cheeseworks.ogdps-level-thumbs",
    "name": "OGDPS Level Thumbnails",
    "developers": [
        "cdc",
        "Cheeseworks"
    ],
    "repository": "https://github.com/BlueWitherer/ogdps-level-thumbs",
	"issues": {
		"info": "Please report issues to the GitHub repository.",
		"url": "https://github.com/BlueWitherer/ogdps-level-thumbs/issues"
	},
    "description": "Adds thumbnails to levels in search.",
    "resources": {
        "sprites": [
            "resources/*.png"
        ]
    },
    "settings": {
        "thumbnailButton": {
            "name": "Enable Thumbnail Button",
            "description": "Adds a button on the level page that lets you view the thumbnail of the level you are currently on.",
            "type": "bool",
            "default": false
        },
        "cache-limit": {
            "name": "Cache Limit",
            "description": "The amount of thumbnails that are cached in memory.",
            "type": "int",
            "min": 0,
            "default": 50
        }
    },
    "dependencies": [
        {
            "id": "cheeseworks.ogdps-util",
            "importance": "required",
            "version": ">=1.3.0"
        },
        {
            "id": "geode.node-ids",
            "version": ">=1.13.0",
            "importance": "required"
        }
    ]
}